<section class="downloads_page">
  <div class="container">
    <div class="row">
  <div class="col s12 m12 l12">
<div class="card-panel">
<%@downloads.each do |download|%>
  <%=link_to url_for(controller: "home", action: "download", filename: download.download_file) do %>
  <%=download.file_name%> <i class="fa fa-download" aria-hidden="true"></i>
  <%end%>
  <hr>

<%end%>
</div>
  </div>
    </div>
  </div>
</section>
